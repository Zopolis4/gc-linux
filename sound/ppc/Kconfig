# ALSA PowerMac drivers

menu "ALSA PPC devices"
	depends on SND!=n && PPC

config SND_POWERMAC
	tristate "PowerMac (AWACS, DACA, Burgundy, Tumbler, Keywest)"
	depends on SND

config SND_GAMECUBE
	tristate "GameCube audio"
	depends on SND
	help
	  Say 'Y' or 'M' to include support for non-working looping random noise.

endmenu

